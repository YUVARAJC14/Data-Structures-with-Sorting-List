#include <stdio.h>       
int main()    
{         
    int arr[] = {1, 30, 3, 3, 4, 7, 1, 10, 25};   
    int length = sizeof(arr)/sizeof(arr[0]);   
    printf("Duplicate elements in given array: \n"); 
    for(int i = 0; i < length; i++) 
    {    
      for(int j = i + 1; j < length; j++) 
      {    
        if(arr[i] == arr[j])    
        printf("%d\n", arr[j]);    
      }    
    }    
    return 0;    
}
